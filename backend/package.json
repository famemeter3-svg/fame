{
  "name": "taiwan-celebrity-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Taiwan Celebrity Tracker - REST API for managing celebrity data, mentions, and metrics",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "seed": "node scripts/seed-celebrities.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:database": "jest --testNamePattern='Database Integration'",
    "test:backend": "jest --testNamePattern='Backend API Integration'",
    "test:e2e": "jest --testNamePattern='End-to-End'",
    "test:performance": "jest --testNamePattern='Performance'",
    "test:security": "jest --testNamePattern='Security'",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "celebrity",
    "tracker",
    "taiwan",
    "scraping",
    "express",
    "mysql"
  ],
  "author": "Taiwan Celebrity Tracker Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
